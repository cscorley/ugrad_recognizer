PROGRAM STATS
VAR (**)
BEGIN

 (*               *)(**)(**)
    SUM2SUM := +1;
    $SUMSQ := -5a00;
    SU%MSQ := -500;
    FOR I := 1 TO 100 DO
        BEGIN
            READ(VALUE);
            SUM := SUM + VALUE;
            SUMSQ := SUMSQ + VALUE * VALUE
        END;
    MEAN := SUM DIV 100;
    VARI(*ANCE := SUMSQ DIV 100 - MEAN * MEAN;
    WRITE(MEAN,VARIANCE)*);
END.
